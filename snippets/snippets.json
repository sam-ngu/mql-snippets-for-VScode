// Common MQL4 code snippets
   "for forward": {
      "prefix": "for",
      "body": [
         "for(int i=0;i < ${1:int max_iter};i++)",
         "{",
         "   $0",
         "}"
      ],
      "description": "mql4 orderselect loop"
   },
   "for backward": {
      "prefix": "for",
      "body": [
         "for(int i=${1:int max_iter} - 1;i >= 0 ;i--)",
         "{",
         "   $0",
         "}"
      ],
      "description": "mql4 orderselect loop"
   },
   "for OrdersTotal": {
